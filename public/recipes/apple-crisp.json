  "level": "lite",
  "profile": "lite",
  "stacks": {},
  "name": "Apple Crisp",
  "ingredients": [
    "4 cups apples – about 4 apples",
    "2/3 to ¾ cup brown sugar packed",
    "½ cup all-purpose flour",
    "¾ cup oats",
    "¾ tsp. cinnamon",
    "¾ tsp. nutmeg",
    "1/3 cup butter softened"
  ],
  "instructions": [
    "Heat oven to 375",
    "Butter 8x8 square pan",
    "Place apple slices in pan",
    "Mix rest ingredients thoroughly sprinkle on top.",
    "Bake 30 minutes or until brown"
  ],
  "metadata": {
    "source": "Bowman Family Cookbook (2011)",
    "originalTitle": "APPLE CRISP"
  }
}