  "$schema": "https://soustack.spec/soustack.schema.json",
  "level": "lite",
  "profile": "lite",
  "stacks": {},
  "name": "Apple Brownies",
  "ingredients": [
    "½ cup butter",
    "1 cup sugar",
    "1 egg",
    "½ cup nuts",
    "3 medium apples sliced thin",
    "1 cup flour",
    "½ tsp. baking powder",
    "½ tsp. baking soda",
    "¼ tsp. salt",
    "1 tsp. cinnamon",
    "350 for 40-45 minutes."
  ],
  "instructions": [
    "Mix butter, sugar & eggs, beat well, stir in apples, and add nuts.",
    "Sift dry ingredients and blend with apple mixture.",
    "Bake in 9x9 greased pan"
  ],
  "metadata": {
    "source": "Bowman Family Cookbook (2011)",
    "originalTitle": "APPLE BROWNIES"
  }
}